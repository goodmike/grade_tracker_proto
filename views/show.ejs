<p>Data Table</p>
<table>
<col class="date">
<col class="assessment">
<col class="topic">
<col class="score">
<col class="weight">
<col class="weighted_score">
<col class="notes">
<tr>
    <th>Date</th>
    <th>Assessment</th>
    <th>Topic</th>
    <th>Score</th>
    <th>Weight</th>
    <th>Weighted Score</th>
    <th>Notes</th>
</tr>
<% for(var i=0; i < items.length; i++) {
   var grade = items[i] %>
<tr>
    <td><%= grade.date %></td>
    <td><%= grade.assessment %></td>
    <td><%= grade.topic %></td>
    <td><%= grade.score %></td>
    <td><%= grade.weight %></td>
    <td><%= grade.weighted_score %></td>
    <td><%= grade.notes %></td>
</tr>
<% } %>
</table>

<div id="data_doc">This is where the data doc JSON will display.</div>